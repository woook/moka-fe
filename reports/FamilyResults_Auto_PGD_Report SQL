SELECT [dbo_Patient_Table_1].[LastName] & " " & [dbo_Patient_Table_1].[FirstName] AS MName, Left([dbo_Gender_Table_1].[Gender],1) AS G1, dbo_Patient_Table_1.DoB, dbo_Patient_Table_1.PatientTrustID, [dbo_Patient_Table_2].[LastName] & " " & [dbo_Patient_Table_2].[FirstName] AS PName, Left([dbo_Gender_Table_2].[Gender],1) AS G2, dbo_Patient_Table_2.DoB, dbo_Patient_Table_2.PatientTrustID, Patients.CandidateAbnormality, ePGDCycle.ePGDCycleID AS ePCI1, ePGDCycle.IVFNo, ePGDCycle.EggCollectionDate, ePGDCycle.CycleNo, ePGDCycle.PGDResultComment, Patients_1.CandidateAbnormality, ePGDCycle_1.ePGDCycleID AS ePCI2, ePGDCycle_1.IVFNo, ePGDCycle_1.EggCollectionDate, ePGDCycle_1.CycleNo, ePGDCycle_1.PGDResultComment, dbo_Patient_Table.PatientTrustID, Left([dbo_Patient_Table].[PatientTrustID],6) AS Family, Checker.Initials AS [Analyst 1], Checker_1.Initials AS [Analyst 2], ArrayTest.ArrayResultDate AS [Result Date], ArrayTest.ArrayTestID
FROM ((((((((dbo_Patient_Table INNER JOIN dbo_Patient_Table AS dbo_Patient_Table_1 ON dbo_Patient_Table.MotherID = dbo_Patient_Table_1.PatientID) INNER JOIN dbo_Patient_Table AS dbo_Patient_Table_2 ON dbo_Patient_Table.FatherID = dbo_Patient_Table_2.PatientID) INNER JOIN Patients ON dbo_Patient_Table_1.PatientTrustID = Patients.PatientID) INNER JOIN Patients AS Patients_1 ON dbo_Patient_Table_2.PatientTrustID = Patients_1.PatientID) LEFT JOIN ePGDCycle ON Patients.InternalPatientID = ePGDCycle.CarrierInternalPatientID) LEFT JOIN ePGDCycle AS ePGDCycle_1 ON Patients_1.InternalPatientID = ePGDCycle_1.CarrierInternalPatientID) INNER JOIN dbo_Gender_Table AS dbo_Gender_Table_1 ON dbo_Patient_Table_1.GenderID = dbo_Gender_Table_1.GenderID) INNER JOIN dbo_Gender_Table AS dbo_Gender_Table_2 ON dbo_Patient_Table_2.GenderID = dbo_Gender_Table_2.GenderID) INNER JOIN (Patients AS Patients_2 INNER JOIN ((ArrayTest INNER JOIN Checker ON ArrayTest.Check1ID = Checker.Check1ID) INNER JOIN Checker AS Checker_1 ON ArrayTest.Check2ID = Checker_1.Check1ID) ON Patients_2.InternalPatientID = ArrayTest.InternalPatientID) ON dbo_Patient_Table.PatientTrustID = Patients_2.PatientID;
