SELECT PrimerAmplicon.AmpliconID, Chromosome.Chr, PrimerAmplicon.Start19, PrimerAmplicon.Stop19, PrimerAmplicon.Notes, PrimerAmplicon.ForwardSeq, PrimerAmplicon.ReverseSeq, PrimerAmplicon.Mix, PrimerAmplicon.RTray, PrimerAmplicon.RFreezer, PrimerAmplicon.FGrid, PrimerAmplicon.FTray, PrimerAmplicon.FFreezer, PrimerAmplicon.TestResultNotes, PrimerAmplicon.RGrid, PrimerAmplicon.PrimerName, Item_Rtagname.Item AS [Reverse Tag], Item_Ftagname.Item AS [Forward Tag], PrimerAmplicon.DateOrdered
FROM ((PrimerAmplicon INNER JOIN Chromosome ON PrimerAmplicon.ChromosomeID = Chromosome.ChrID) INNER JOIN Item AS Item_Rtagname ON PrimerAmplicon.RTagName = Item_Rtagname.ItemID) INNER JOIN Item AS Item_Ftagname ON PrimerAmplicon.FTagName = Item_Ftagname.ItemID
WHERE (((PrimerAmplicon.Status)=1202218832));
